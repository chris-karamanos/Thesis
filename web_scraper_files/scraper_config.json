{
    "in.gr": {
        "category": "politics, economy",
        "rss": "https://www.in.gr/feed/",
        "method": "rss",
        "rss_filters": ["Πολιτική", "Ελλάδα", "Υγεία", "Κόσμος", "Οικονομία", "Κυβέρνηση", "Κοινωνία"],
        "caps": [
            { "labels": ["Πολιτική", "Ελλάδα", "Κοινωνία"], "max":5},
            { "labels": ["Οικονομία"],                       "max":5},
            { "labels": ["Κόσμος", "Υγεία", "Κυβέρνηση"],    "max":2}
        ],
        "rss_exclude": ["Σπορ", "Αθλητικά", "Ποδόσφαιρο", "Μπάσκετ", "Super League", "Euroleague", "Eurovision"],
        "fetch_full_text": true,
        "content_root_selector": "article", 
        "dom_exclude_selectors": [
            "div.wrap___headlines",
            "ul.trending-col-posts",
            "div.trending-col-posts",
            "div.trending-col",
            "li.tr_row",                     
            "div#app",                       
            "iframe",                        
            "blockquote.twitter-tweet",    
            "script, style, noscript",
            "aside, [role='complementary']",
            ".share, .sharing, .social",
            "[class*='ad-'], .ad, .advert",
            ".related, [class*='related']",
            "nav, header, footer"
  ]
    },
    "kathimerini.gr": {
        "category": "politics, economy",
        "method": "html",

        "listing_urls": [
            "https://www.kathimerini.gr/politics/",
            "https://www.kathimerini.gr/economy/"
        ],

        "max_pages": 3,
        "max_articles": 10,             
        "max_per_listing": 5,           

        "listing_scope_selector": "main#primary",
        "card_selector": ".card",
        "link_selector": "a[href]",
        "next_page_selector": "a.next",   

        "allow_url_regex": "^https?://(?:www\\.)?kathimerini\\.gr/(politics|economy)/",
        "block_url_regex": "(/video/|/multimedia/|/photo/|/podcast/)",

        "fetch_full_text": true,

        "content_root_selector": "article, .entry-content, .post-content",
        "dom_exclude_selectors": [
            "script, style, noscript",
            "header, footer, nav, aside",
            ".related, [class*='related'], [class*='recirc']",
            "[class*='share'], .social, [class*='social']",
            "[class*='ad-'], .ad, .advert, .sponsored, [id*='banner']"
        ],

        "paywall_selectors": [
            ".paidLabel-text",          
            "[class*='paidLabel']"
        ],
        "paywall_phrases": [
            "ΔΙΑΘΕΣΙΜΟ ΜΕ ΣΥΝΔΡΟΜΗ", "subscription"
        ]
    },	
